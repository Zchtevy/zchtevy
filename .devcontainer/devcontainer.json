{
	"name": "Zchtevy",
	"build": {
		"dockerfile": "Dockerfile",
		"args": {
			"VARIANT": "5.0"
		}
	},
	"settings": {
		"terminal.integrated.shell.linux": "/bin/fish"
	},
	"extensions": [
		"ms-azuretools.vscode-docker"
	],
	"mounts": [
		"source=/var/run/docker.sock,target=/var/run/docker.sock,type=bind"
	],
	"runArgs": [ "--privileged" ],
	"remoteUser": "vscode"
}